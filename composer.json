{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^3.1",
        "bavix/laravel-wallet": "^11.4",
        "bensampo/laravel-enum": "^6.12",
        "binarytorch/larecipe": "^2.8",
        "binarytorch/larecipe-swagger": "^0.2.0",
        "cjmellor/level-up": "^1.4",
        "cmgmyr/messenger": "^2.31",
        "creagia/laravel-sign-pad": "^2.1",
        "cybercog/laravel-ban": "^4.10",
        "cybercog/laravel-love": "^10.1",
        "jenssegers/agent": "^2.6",
        "laravel-notification-channels/webpush": "^10.2",
        "laravel/folio": "^1.1",
        "laravel/framework": "^11.0",
        "laravel/reverb": "^1.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.23",
        "laravel/tinker": "^2.9",
        "laraveldaily/laravel-charts": "^0.2.3",
        "laraveldaily/laravel-invoices": "^4.1",
        "laravolt/avatar": "^6.2",
        "larswiegers/laravel-maps": "^0.19.0",
        "livewire/livewire": "^3.6",
        "livewire/volt": "^1.7",
        "maatwebsite/excel": "^3.1",
        "midtrans/midtrans-php": "^2.6",
        "milwad/laravel-attributes": "^1.3",
        "opcodesio/log-viewer": "^3.17",
        "safemood/discountify": "^1.5",
        "silviolleite/laravelpwa": "^2.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-model-status": "^1.18",
        "spatie/laravel-permission": "^6.20",
        "spatie/laravel-sitemap": "^7.3",
        "wendelladriel/laravel-lift": "^0.15.0",
        "wildside/userstamps": "^3.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
